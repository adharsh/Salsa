  Enchufa -> "Dile Que No Position";
  "Dile Que No Position" -> Guapea;
  "Dile Que No Position" -> Exhibe;
  "Dile Que No Position" -> Pasea;
  "Dile Que No Position" -> Pasea;
  Pasea -> Guapea;
  Exhibe -> "Dile Que No Position";
  
  // Closed Position - usually only in beginning, so commented out
