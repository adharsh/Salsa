  Guapea -> "Right-to-Right";
  "Right-to-Right" -> "Medio Sombrero" -> "Dile Que No Position";
  "Dile Que No Position" -> "Exhibe (Right-to-Right)" -> "Dile Que No Position";

  // Level 3b
  "Right-to-Right" -> Sombrero -> Guapea;
